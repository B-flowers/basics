<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ofs.sys.mapper.SysUserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ofs.sys.entity.SysUser">
        <result column="ID" property="id"/>
        <result column="LOGIN_ID" property="loginId"/>
        <result column="PASSWORD" property="password"/>
        <result column="NAME" property="name"/>
        <result column="MOBILE" property="mobile"/>
        <result column="EMAIL" property="email"/>
        <result column="GROUP_ID" property="groupId"/>
        <result column="LAST_LOGIN_DATE" property="lastLoginDate"/>
        <result column="CREATE_DATE" property="createDate"/>
        <result column="UPDATE_DATE" property="updateDate"/>
        <result column="CREATE_USER" property="createUser"/>
        <result column="UPDATE_USER" property="updateUser"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
                ID,
            login_id, password, name, mobile, email, group_id, last_login_date, CREATE_DATE, UPDATE_DATE, CREATE_USER, UPDATE_USER
        </sql>

</mapper>
