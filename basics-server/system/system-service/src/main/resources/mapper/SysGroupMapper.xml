    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        T.ID, T.CODE, T.NAME, T.DESCRIPTION, T.CREATE_DATE, T.UPDATE_DATE, T.CREATE_USER, T.UPDATE_USER
